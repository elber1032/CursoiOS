//: Playground - noun: a place where people can play

import UIKit


//Reto Clase

for num in 1...100 {
    
    
    if num % 5 == 0 {
        print(num, "Bingo")
    }else if num % 2 == 0 {
        print(num, "Par")
    }else if num % 2 != 0 {
        print(num, "Impar")
        
    }else if num >= 30 && num <= 40 {
        print(num, "Viva Swift")
    }else{
        print("Error")
    }
    
    
    //print(num)
}
